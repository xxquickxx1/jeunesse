<script type="text/javascript">


$(document).ready(function(){

$(document).scroll(function(){
  if($(this).scrollTop() > 300){
    $(".top-bar-section ul li.last a").addClass('button success bordered');
  }
});

$(document).scroll(function() {
    if ($(this).scrollTop() < 100) {
       $('.top-bar-section ul li:nth-last-child(n+2)').addClass('animated fadeOutLeft');
       $('.top-bar-section ul li.last a').removeClass('button success bordered');
        }
   
});

$(document).scroll(function(){
  if($(this).scrollTop() == 0){
   $('.top-bar-section ul li:nth-last-child(n+2)').removeClass('fadeOutLeft').addClass('fadeInLeft');
    
  }
});

});

</script>


